package myBasicCalculator;

public class Calculator 
{	
	int answer;
	Operation lastOperation;
	
	public Calculator(int answer) 
	{
		this.answer =answer;
		}

	public enum Operation 
	{
		ADD,
		SUBTRACT,
		MULTIPLAY,
		DIVIDE;
	}
	public void sum(int num1, int num2)
	{
		answer = num1+num2;
		lastOperation = Operation.ADD;
	}

}
